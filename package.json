{
	"name": "lyrictypesk",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"npm run dev:vite\" \"npm run dev:emulators\" --names \"SvelteKit,Firebase\" --prefix-colors \"cyan,yellow\"",
		"dev:split": "osascript -e 'tell application \"Terminal\" to do script \"cd $(pwd) && npm run dev:vite\"' && osascript -e 'tell application \"Terminal\" to do script \"cd $(pwd) && npm run dev:emulators\"'",
		"dev:vite": "vite dev",
		"dev:emulators": "firebase emulators:start --only hosting,functions",
		"dev:vite-only": "vite dev",
		"dev:emulators-only": "firebase emulators:start --only hosting,functions",
		"build": "vite build",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"concurrently": "^9.2.0",
		"firebase-tools": "^14.16.0",
		"svelte": "^4.2.7",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@thi.ng/pixel": "^7.3.14",
		"@thi.ng/pixel-dither": "^1.1.154",
		"axios": "^1.10.0",
		"canvas": "^3.1.2",
		"cheerio": "^1.0.0-rc.12",
		"cors": "^2.8.5",
		"firebase": "^10.8.0",
		"js-cookie": "^3.0.5",
		"normalize-text": "^2.5.0",
		"pako": "^2.1.0",
		"robocopy": "^0.1.16",
		"textfit": "^2.4.0",
		"unidecode": "^1.1.0"
	}
}
